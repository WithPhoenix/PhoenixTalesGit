{
  "multipart": [
    {
      "apply": {
        "model": "phoenixtales:block/soldering_table"
      }
    },
    {
      "when": {
        "facing": "north",
        "connected": "true"
      },
      "apply": {
        "model": "phoenixtales:block/s_adapter",
        "uvlock": false
      }
    },
    {
      "when": {
        "facing": "south",
        "connected": "true"
      },
      "apply": {
        "model": "phoenixtales:block/s_adapter",
        "y": 180,
        "uvlock": false
      }
    },
    {
      "when": {
        "facing": "east",
        "connected": "true"
      },
      "apply": {
        "model": "phoenixtales:block/s_adapter",
        "y": 90,
        "uvlock": false
      }
    },
    {
      "when": {
        "facing": "west",
        "connected": "true"
      },
      "apply": {
        "model": "phoenixtales:block/s_adapter",
        "y": 270,
        "uvlock": false
      }
    },
    {
      "when": {
        "facing": "north",
        "tin_solder": "true"
      },
      "apply": {
        "model": "phoenixtales:block/s_tin",
        "uvlock": false
      }
    },
    {
      "when": {
        "facing": "south",
        "tin_solder": "true"
      },
      "apply": {
        "model": "phoenixtales:block/s_tin",
        "y": 180,
        "uvlock": false
      }
    },
    {
      "when": {
        "facing": "east",
        "tin_solder": "true"
      },
      "apply": {
        "model": "phoenixtales:block/s_tin",
        "y": 90,
        "uvlock": false
      }
    },
    {
      "when": {
        "facing": "west",
        "tin_solder": "true"
      },
      "apply": {
        "model": "phoenixtales:block/s_tin",
        "y": 270,
        "uvlock": false
      }
    },
    {
      "when": {
        "facing": "north",
        "soldering_iron": "true"
      },
      "apply": {
        "model": "phoenixtales:block/s_iron",
        "uvlock": false
      }
    },
    {
      "when": {
        "facing": "south",
        "soldering_iron": "true"
      },
      "apply": {
        "model": "phoenixtales:block/s_iron",
        "y": 180,
        "uvlock": false
      }
    },
    {
      "when": {
        "facing": "east",
        "soldering_iron": "true"
      },
      "apply": {
        "model": "phoenixtales:block/s_iron",
        "y": 90,
        "uvlock": false
      }
    },
    {
      "when": {
        "facing": "west",
        "soldering_iron": "true"
      },
      "apply": {
        "model": "phoenixtales:block/s_iron",
        "y": 270,
        "uvlock": false
      }
    }
  ]
}