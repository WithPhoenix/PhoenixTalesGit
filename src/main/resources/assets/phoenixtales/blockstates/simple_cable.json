{
  "multipart": [
    {
      "apply": {
        "model": "phoenixtales:block/simple_cable"
      }
    },
    {
      "when": {
        "north": "true"
      },
      "apply": {
        "model": "phoenixtales:block/cable_add",
        "uvlock": false
      }
    },
    {
      "when": {
        "east": "true"
      },
      "apply": {
        "model": "phoenixtales:block/cable_add",
        "y": 90,
        "uvlock": false
      }
    },
    {
      "when": {
        "south": "true"
      },
      "apply": {
        "model": "phoenixtales:block/cable_add",
        "y": 180,
        "uvlock": false
      }
    },
    {
      "when": {
        "west": "true"
      },
      "apply": {
        "model": "phoenixtales:block/cable_add",
        "y": 270,
        "uvlock": false
      }
    },
    {
      "when": {
        "up": "true"
      },
      "apply": {
        "model": "phoenixtales:block/cable_add",
        "x": 270,
        "uvlock": false
      }
    },
    {
      "when": {
        "down": "true"
      },
      "apply": {
        "model": "phoenixtales:block/cable_add",
        "x": 90,
        "uvlock": false
      }
    }
  ]
}
